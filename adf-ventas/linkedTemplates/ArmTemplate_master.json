{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-ventas"
		},
		"AzureDatabricks_LinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_LinkedService\""
		},
		"AzureSynapseAnalytics1_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"AzureSynapseAnalytics1\""
		},
		"datalakehouse_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"datalakehouse\""
		},
		"AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1012-001337-6jboijwp"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tcp:synapseventas1.sql.azuresynapse.net,1433"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "data_ventas"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"datalakehouse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datacsv03.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ventas_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks_LinkedService_accessToken": {
						"value": "[parameters('AzureDatabricks_LinkedService_accessToken')]"
					},
					"AzureSynapseAnalytics1_password": {
						"value": "[parameters('AzureSynapseAnalytics1_password')]"
					},
					"datalakehouse_accountKey": {
						"value": "[parameters('datalakehouse_accountKey')]"
					},
					"AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_LinkedService_properties_typeProperties_existingClusterId')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_userName')]"
					},
					"datalakehouse_properties_typeProperties_url": {
						"value": "[parameters('datalakehouse_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}